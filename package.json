{
  "name": "qullect-backend-api",
  "version": "1.0.0",
  "main": "src/Bootstrap.ts",
  "author": "Emmanuel Onyo <emmanuelonyo34@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "NODE_PATH=./dist node dist/Bootstraps.js",
    "build": "tsc -p .",
    "postbuild": "ts-node scripts/copy-assets",
    "db:migrate": "npx prisma migrate deploy --schema=./src/core/db/prisma",
    "db:generate": "npx prisma generate --schema=./src/core/db/prisma --no-engine",
    "db:push": "npx prisma db push --schema=./src/core/db/prisma --skip-generate",
    "db:studio": "npx prisma studio --schema=./src/core/db/prisma --port 5556",
    "dev": "nodemon -r tsconfig-paths/register ./src/Bootstraps.ts",
    "with-dev-env": "dotenv -e .env --",
    "setup": "node scripts/setup.mjs make:certs ./dist/core/.certs",
    "setup:dev": "node scripts/setup.mjs make:certs ./src/core/.certs"
  },
  "_moduleAliases": {
    "@": "./src",
    "@core": "./src/core",
    "@modules": "./src/Modules",
    "@utils": "./src/utils",
    "@types": "./src/types",
    "@config": "./src/config",
    "@services": "./src/services",
    "@middlewares": "./src/middlewares"
  },
  "dependencies": {
    "@azure-rest/ai-vision-face": "^1.0.0-beta.2",
    "@azure/identity": "^4.5.0",
    "@prisma/client": "^6.10.1",
    "@sentry/node": "^9.40.0",
    "amqplib": "^0.10.8",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.5.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.4",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.1",
    "express-jwt": "^8.4.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.16",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.10.1",
    "redis": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.9.1",
    "@types/nodemailer": "^6.4.17",
    "@types/shelljs": "^0.8.15",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.15.0",
    "globals": "^15.12.0",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.7",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0"
  }
}
